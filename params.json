{
    "pop_size": 100,
    "max_depth": 5,
    "random_gen":
    {
        "fill_proba": 0.5,

        "bin_op":
        [
            { "proba":0.25, "name":"+", "fun":"x+y" },
            { "proba":0.20, "name":"-", "fun":"x-y" },
            { "proba":0.25, "name":"*", "fun":"x*y" },
            { "proba":0.20, "name":"/", "fun":"x/y" },
            { "proba":0.10, "name":"^", "fun":"x^y" }
        ],
        "bin_proba": 0.80,

        "un_op":
        [
            { "proba":0.2, "name":"cos", "fun":"cos(x)" },
            { "proba":0.2, "name":"sin", "fun":"sin(x)" },
            { "proba":0.2, "name":"tan", "fun":"tan(x)" },
            { "proba":0.2, "name":"ln", "fun":"ln(x)" },
            { "proba":0.2, "name":"exp", "fun":"exp(x)" }
        ],
        "un_proba": 0.10,

        "const_range": { "min":-5.0, "max":5.0 },
        "const_proba": 0.05,
        "var_proba": 0.05
    },
    "growth_factor": 2.0,
    "mutation_ratio": 0.1
}
